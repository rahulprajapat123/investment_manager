# Environment Variables Template
# Copy this to .env and fill in your values

# GCP Settings
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1

# API Keys
NEWS_API_KEY=your-news-api-key
ALPHA_VANTAGE_KEY=your-alpha-vantage-key

# Feature Flags
ENABLE_ML=true
ENABLE_NOTIF=true
USE_LOCAL_DATA=true

# Performance
MAX_WORKERS=10
QUERY_TIMEOUT=60

# API Settings
API_HOST=0.0.0.0
API_PORT=8080
DEBUG=false
